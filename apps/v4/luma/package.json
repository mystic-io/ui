    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "chromatic": "^11.18.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.9",
    "pixelmatch": "^6.0.0",
    "playwright": "^1.56.1",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.49",
    "puppeteer": "^23.10.4",
    "puppeteer-extra": "^3.3.6",
