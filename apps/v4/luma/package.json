{
  "name": "luma-design-system-reverse-engineering",
  "version": "1.0.0",
  "description": "Autonomous reverse engineering system for Luma style guide using shadcn/ui and Radix primitives",
  "type": "module",
  "scripts": {
    "reverse-engineer:luma": "node orchestrator.js",
    "extract": "node extract-luma-styles.js",
    "extract:screenshots": "node capture-screenshots.js",
    "compare": "node compare-with-luma.js",
    "compare:watch": "nodemon --watch src/components --exec 'node compare-with-luma.js'",
    "compare:detailed": "node compare-with-luma.js --detailed",
    "validate": "node validate-components.js",
    "report": "node generate-report.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:visual": "playwright test --grep @visual",
    "test:a11y": "playwright test --grep @a11y",
    "test:all": "pnpm test && pnpm test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.2",
